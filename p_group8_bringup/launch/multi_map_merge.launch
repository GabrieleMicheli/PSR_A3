<launch>

  <!-- Merging RED Team maps -->
  <arg name="red_first_tb3"  default="R1"/>
  <arg name="red_second_tb3" default="R2"/>
  <arg name="red_third_tb3"  default="R3"/>

  <arg name="red_first_tb3_x_pos" default="3.0"/>
  <arg name="red_first_tb3_y_pos" default="5.0"/>
  <arg name="red_first_tb3_z_pos" default="0.0"/>
  <arg name="red_first_tb3_yaw"   default="0.0"/>

  <arg name="red_second_tb3_x_pos" default="4.0"/>
  <arg name="red_second_tb3_y_pos" default="5.0"/>
  <arg name="red_second_tb3_z_pos" default="0.0"/>
  <arg name="red_second_tb3_yaw"   default="0"/>

  <arg name="red_third_tb3_x_pos" default="5.0"/>
  <arg name="red_third_tb3_y_pos" default="5.0"/>
  <arg name="red_third_tb3_z_pos" default="0.0"/>
  <arg name="red_third_tb3_yaw"   default="0.0"/>

  <group ns="$(arg red_first_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg red_first_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg red_first_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg red_first_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg red_first_tb3_yaw)"  />
  </group>

  <group ns="$(arg red_second_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg red_second_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg red_second_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg red_second_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg red_second_tb3_yaw)"  />
  </group>

  <group ns="$(arg red_third_tb3)/map_merge">
    <param name="init_pose_x"   value="$(arg red_third_tb3_x_pos)"/>
    <param name="init_pose_y"   value="$(arg red_third_tb3_y_pos)"/>
    <param name="init_pose_z"   value="$(arg red_third_tb3_z_pos)"/>
    <param name="init_pose_yaw" value="$(arg red_third_tb3_yaw)"  />
  </group>

